
	<title>Shoping Cart</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">



<header class="header-v3 ">

			<div class="wrap-menu-desktop how-shadow1">
				<nav class="limiter-menu-desktop container">

					<!-- Logo desktop -->
					<a href="/" class="logo">
						<img src="https://enrich4life.com/wp-content/uploads/2019/03/Enrich-CBD-Logo-Transparent.png" alt="IMG-LOGO">
					</a>

					<!-- Menu desktop -->
					<div class="menu-desktop">
						<ul class="main-menu-wide">
							<li>
								<a href="/">Home</a>
							</li>

							<li>
								<a href="/products">Shop</a>
                				<ul class="sub-menu-wide" >
									<li><a href="/products/Edibles">Edibles</a></li>
									<li><a href="/products/Vegan">Vegan</a></li>
									<li><a href="/products/Tincture">Tincture</a></li>
									<li><a href="/products/Pet-care">Pet-care</a></li>
								</ul>
							</li>

							<li>
								<a href="#">About</a>
							</li>

							<li>
								<a href="#">Contact</a>
							</li>
						</ul>
					</div>



					<!-- Icon header -->
					<div id="carty" class="wrap-icon-header flex-w flex-r-m">
            		<ul class="main-menu">
							<li>
								<form action="/product/search" method="POST" class="form-inline">
									<input type="text" id="searchTxt" name="searchTxt" class=" form-control-sm mr-3 w-75 input1 bg-none plh1 stext-107 cl7" placeholder="Search for product">
  								<i class="cl0 fas fa-search" aria-hidden="true"></i>
								</form>
							</li>


							<li>
                  				<button href="#" class="icon-header-item cl0 hov-cl1 trans-04 p-l-22 p-r-11" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
      							<i class="zmdi zmdi-account"></i>
      						</button>
                  			<ul class="sub-menu">
                    			{{# if login}}
  									<li><a href="/users/profile">Profile</a></li>
  									<li><a href="/users/logout">Logout</a></li>
                    			{{ else }}
                    				<li><a href="/users/signin">Sign In</a></li>
  									<li><a href="/users/signup">Sign Up</a></li>
  									{{/if}}
  								</ul>

							</li>

							<li>
                		<div  class="icon-header-item cl0 hov-cl1 trans-04 p-l-22 p-r-11 {{# if session.cart.totalQty}}icon-header-noti{{/if}} js-show-cart" data-notify={{session.cart.totalQty}}>
    							<i class="zmdi zmdi-shopping-cart" onclick="location.replace('/shopping-cart');"></i>
    			</div>
							</li>
						</ul>
					</div>
				</nav>
</div>
<!-- Header Mobile -->
<div id="carty-mobile">
		<div  class="wrap-header-mobile">
			<!-- Logo moblie -->
			<div class="logo-mobile">
				<a href="/"><img src="https://enrich4life.com/wp-content/uploads/2019/03/Enrich-CBD-Logo-Transparent.png" alt="IMG-LOGO"></a>
			</div>

			<!-- Icon header -->
			<div class="wrap-icon-header flex-w flex-r-m m-r-15">
				<div class="icon-header-item cl0 hov-cl1 trans-04 p-r-11 js-show-modal-search">
					<i class="zmdi zmdi-search"></i>
				</div>

				<div class="icon-header-item cl0 hov-cl1 trans-04 p-r-11 p-l-10 {{#if session.cart.totalQty}}icon-header-noti{{/if}} js-show-cart" data-notify={{session.cart.totalQty}}>
					<i class="zmdi zmdi-shopping-cart" onclick="location.replace('/shopping-cart');"></i>
				</div>
			</div>

			<!-- Button show menu -->
			<div class="btn-show-menu-mobile  hamburger hamburger--squeeze">
				<span class="hamburger-box">
					<span class="hamburger-inner"></span>
				</span>
			</div>
		</div>


		<!-- Menu Mobile -->
		<div class="menu-mobile">
			<ul class="topbar-mobile">

				<li>
					<div class="right-top-bar flex-w h-full">
						{{#if login}}
						<a href="/users/profile" class="flex-c-m p-lr-10 trans-04">
							Profile
						</a>

						<a href="/users/logout" class="flex-c-m p-lr-10 trans-04">
							Logout
						</a>

						{{ else }}

						<a href="/users/signin" class="flex-c-m p-lr-10 trans-04">
							Sign In
						</a>

						<a href="/users/signup" class="flex-c-m p-lr-10 trans-04">
							Sign Up
						</a>

						{{/if}}
					</div>
				</li>
			</ul>

			<ul class="main-menu-m">
				<li>
					<a href="/">Home</a>
				</li>

				<li>
					<a href="/products">Shop</a>
					<ul class="sub-menu-m">
						<li><a href="/products/Edibles">Edibles</a></li>
						<li><a href="/products/Vegan">Vegan</a></li>
						<li><a href="/products/Tincture">Tincture</a></li>
						<li><a href="/products/Pet-care">Pet-care</a></li>
					</ul>
					<span class="arrow-main-menu-m">
						<i class="fa fa-angle-right" aria-hidden="true"></i>
					</span>
				</li>

				

				<li>
					<a href="/">About</a>
				</li>

				<li>
					<a href="/">Contact</a>
				</li>
			</ul>
		</div>

		
<div class="modal-search-header flex-c-m trans-04 js-hide-modal-search">
			<div class="container-search-header">
				<button class="flex-c-m btn-hide-modal-search trans-04 js-hide-modal-search">
					<img src="/test/images/icons/icon-close2.png" alt="CLOSE">
				</button>

				<form action="/product/search" method="POST" class="wrap-search-header flex-w p-l-15">
					<button class="flex-c-m trans-04">
						<i class="zmdi zmdi-search"></i>
					</button>
					<input class="plh3" type="text" id="searchTxt" name="searchTxt" placeholder="Search...">
				</form>
			</div>
		</div>
		</div>
</header>




		
